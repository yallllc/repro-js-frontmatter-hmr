{
  "name": "gatsby-transformer-javascript-frontmatter",
  "description": "Gatsby transformer plugin for JavaScript to extract exports.frontmatter statically.",
  "version": "4.17.0-next.0",
  "author": "Jacob Bolda <me@jacobbolda.com>",
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-transformer-javascript-frontmatter#readme",
  "dependencies": {
    "@babel/parser": "^7.15.5",
    "@babel/runtime": "^7.15.4",
    "@babel/traverse": "^7.15.4",
    "bluebird": "^3.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "babel-preset-gatsby-package": "^2.17.0-next.0",
    "cross-env": "^7.0.3"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "js"
  ],
  "license": "MIT",
  "peerDependencies": {
    "gatsby": "^4.0.0-next",
    "gatsby-source-filesystem": "^4.0.0-next"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-transformer-javascript-frontmatter"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\""
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
